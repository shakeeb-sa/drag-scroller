{
  "manifest_version": 3,
  "name": "React Drag Scroller",
  "version": "1.0",
  "description": "Hold Right Click to drag-scroll any container.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}